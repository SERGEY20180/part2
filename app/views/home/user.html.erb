<h1>Все пользователи</h1>
<table class="table table-striped">
  

  <tbody>
      <% @users.each do |user| %>
        <tr class="bg-success">
          <td><%= user. email %></td>
          <td><%= link_to 'Show', home_index_path %></td>
          <td><%= link_to 'Edit', home_index_path %></td>
          <td><%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
  </tbody>
</table>
<br>


